   "source": [
    "max_num = max(m,n)\n",
    "\n",
    "for i in range(1, max_num+1):\n",
    "    if m % i == 0 and n % i == 0:\n",
    "        gcd = i\n",
    "print(gcd)\n",
    "\n",
    "a = m//gcd\n",
    "b = n//gcd\n",
    "lcd = int(a * b * gcd)\n",
    "print(lcd)\n"
   ]
