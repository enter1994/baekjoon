   "source": [
    "T = int(input())\n",
    "\n",
    "for i in range(T):    \n",
    "    # h : 호텔의 층수 w : 각층의 방수 n : 몇번째 손님\n",
    "    h, w, n = map(int, input().split())\n",
    "    \n",
    "    floor = n%h\n",
    "    num = n//h +1\n",
    "    \n",
    "    if floor == 0:\n",
    "        floor = h\n",
    "        num = n//h\n",
    "        \n",
    "    print(floor*100 + num)"
   ]
